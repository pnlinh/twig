{
    "name": "handinh/twig",
    "description": "Twig Engine for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Dinh Quoc Han",
            "email": "dinhquochan96@gmail.com"
        }
    ],
    "require": {
        "php": ">7.1.5",
        "twig/twig": "~2.5.0",
        "illuminate/support": "5.5.*|5.6.*|5.7.*",
        "illuminate/view": "5.5.*|5.6.*|5.7.*"
    },
    "require-dev": {
        "laravel/framework": "~5.7.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Qh\\Twig\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Qh\\Twig\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
